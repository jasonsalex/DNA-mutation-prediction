{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "C/C++: Clang++ 生成活动文件",
            "type": "cppbuild",
            "command": "/usr/bin/clang++",
            "args": [
                "-std=c++17",
                "-arch","arm64",
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-g",
                "-I", "libtorch/include",
                "-I", "libtorch/include/torch/csrc/api/include",
                "-L", "libtorch/lib",
                "-Wl,-rpath,libtorch/lib",
                "${file}",
                "-ltorch",
                "-ltorch_cpu",
                "-lc10",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ]
}